" -----------------------------------------------------------------------------
"  < Syntastic 插件配置 >
" -----------------------------------------------------------------------------
" 用于保存文件时查检语法
"Check when load file (default: 0)
"let g:syntastic_check_on_open=1       "打开后每次会变卡
"Set error and warning symbols...
"...syntax errors (default: '>>')
let g:syntastic_error_symbol='>X'
"...syntax warnings (default: '>>')
let g:syntastic_warning_symbol='>!'
"...style error (default: 'S>')
let g:syntastic_style_error_symbol='SX'
"...style warning (default: 'S>'
let g:syntastic_style_warning_symbol='S!'
"Set checker for python filetype
let g:syntastic_python_checkers = ['pylint']
"Disable annoying pylint mesages
"   C0103 Invalid constant name
"   C0111 Missing method docstring
"   R0903 Too few public methods (Classes aren't meant to just store datas.)
"   R0913 Too many arguments
"   W0141 Used builtin function 'map'|'filter' (Why not? Nonsens!)
"   W0142 Used * or ** magic (sometimes it's unavoidable)
"   W0511 'TODO' detected
let g:syntastic_python_pylint_post_args='--disable=C0103,C0111,R0903,R0913,W0141,W0142,W0511'
"Syntastic toggle error location-list
map <silent> Te :call ToggleErrorMode()<CR>
function! ToggleErrorMode()
  if !exists('b:error_mode')
    let b:error_mode=0
  endif
  if b:error_mode
    let b:error_mod e=0
    unmap n
    unmap p
    map q :q<CR>
    lclose
  else
    let b:error_mod e=1
    map <silent> n :lnext<CR>
    map <silent> p :lprev<CR>
    map <silent> q :call ToggleErrorMode()<CR>
    Errors
    "Jump to first entry. Needed if only one entry exists.
    "Otherwise `lnext` and `lprev` don't jump to this
    "single entry.
    lrewind
  endif
endfunction

let g:syntastic_auto_loc_list = 1
let g:syntastic_loc_list_height = 5
let g:syntastic_enable_highlighting = 1
let g:syntastic_javascript_checkers = ['jshint']

nmap <leader>sr :SyntasticReset<CR>

